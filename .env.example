# Back4App Parse Server Configuration
NEXT_PUBLIC_PARSE_APP_ID=your_app_id_here
NEXT_PUBLIC_PARSE_JS_KEY=your_js_key_here
NEXT_PUBLIC_PARSE_SERVER_URL=https://parseapi.back4app.com

# Master Key (server-side only - NEVER expose to client)
# Only needed for admin scripts, not for the web app
PARSE_MASTER_KEY=your_master_key_here

# Back4App Personal Access Token
# Only needed for deployment scripts
BACK4APP_ACCOUNT_KEY=your_account_key_here

# App Configuration
NEXT_PUBLIC_APP_URL=https://your-app-name.vercel.app

# Optional: Twilio SMS (for future notifications)
# TWILIO_ACCOUNT_SID=your_account_sid
# TWILIO_AUTH_TOKEN=your_auth_token
# TWILIO_PHONE_NUMBER=your_twilio_number

# Optional: Email Service (for future notifications)
# SENDGRID_API_KEY=your_sendgrid_key
# or
# RESEND_API_KEY=your_resend_key

# Optional: Firebase (for push notifications)
# NEXT_PUBLIC_FIREBASE_CONFIG=your_firebase_config_json
